
select c.entityid || ' ' || c.firstname || ' ' || c.lastname as customer_name, ca.addr1 || ' ' || ca.city || ' ' || ca.state || ' ' || ca.zip as customer_address, c.email as customer_email, count(i.id) as invoices_count, c.custentity_click_id as gclid, BUILTIN.DF(c.leadsource) as lead_source, tl.rate as advance_amount
from customer c
LEFT JOIN entityaddressbook ab on ab.entity=c.id
LEFT JOIN entityaddress ca on ca.nkey=ab.addressbookaddress
left join transaction t on t.entity=c.id
left join transactionline tl on tl.transaction=t.id
left join transaction i on i.entity=c.id
where c.entityid='23630:1' and t.type='Estimate' and tl.item=7 and t.custbody_preferred_quote='T'
group by c.entityid, c.firstname, c.lastname, ca.addr1, ca.city, ca.state, ca.zip, c.email, c.custentity_click_id, BUILTIN.DF(c.leadsource), tl.rate

